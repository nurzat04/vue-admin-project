{
  "name": "atguigu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "lint:css": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "prepare": "husky",
    "build:test": "vue-tst && vite build --mode test",
    "build:pro": "vue-tst && vite build --mode production"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.8.4",
    "element-plus": "^2.9.7",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "progress": "^2.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^12.0.0-alpha.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-define-options": "^3.0.0-beta.8",
    "vite": "^6.2.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.4"
  },
  "lint-staged": {
    "**/*.{js,vue,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,scss,less}": [
      "stylelint --fix"
    ]
  }
}
